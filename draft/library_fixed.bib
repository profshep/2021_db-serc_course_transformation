isbn = {10.1039/C2CP42975F},

journal = {Phys. Chem. Chem. Phys.},
keywords = {Imidazoles,Imidazoles: chemistry,Ionic Liquids,Ionic Liquids: chemistry,Molecular Structure,Quantum Theory,Water,Water: chemistry},
number = {45},
pages = {15897--908},
pmid = {23093293},
title = {Theoretical and experimental studies of water interaction in acetate based ionic liquids.},
