journal = {Faraday Discuss.},
pages = {493--505},
pmid = {25611039},
title = {Two-dimensional sum-frequency generation (2D SFG) spectroscopy: summary of principles and its application to amyloid fiber monolayers.},
url = {},
volume = {177},
year = {2015}
}
