journal = {Biochim. Biophys. Acta - Biomembr.},
keywords = {Cholesterol,Na K-ATPase phospholipids,Recombinant Na K-ATPase complexes,Reconstitution,Specific lipid-protein interactions,X-ray crystal structure},
number = {9},
pages = {1729--1743},
pmid = {25791351},
publisher = {Elsevier B.V.},
title = {General and specific lipid-protein interactions in Na,K-ATPase},
