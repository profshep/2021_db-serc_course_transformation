pages = {15508--11},
pmid = {20958066},
title = {Vibrational predissociation spectrum of the carbamate radical anion, C(5)H(5)N-CO(2)(-), generated by reaction of pyridine with (CO(2))(m)(-).},
url = {},
volume = {132},
year = {2010}
}
