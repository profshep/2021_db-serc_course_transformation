@article{Shannon1976,
abstract = {The effective ionic radii of Shannon \& Prewitt [{\it Acta Cryst.} (1969), B{\bf 25}, 925-945] are revised to include more unusual oxidation states and coordinations. Revisions are based on new structural data, empirical bond strength-bond length relationships, and plots of (1) radii vs volume, (2) radii {\it vs} coordination number, and (3) radii {\it vs} oxidation state. Factors which affect radii additivity are polyhedral distortion, partial occupancy of cation sites, covalence, and metallic character. Mean Nb${\sp 5+}$-O and Mo${\sp 6+}$-O octahedral distances are linearly dependent on distortion. A decrease in cation occupancy increases mean Li${\sp +}$-O, Na${\sp +}$-O, and Ag${\sp +}$-O distances in a predictable manner. Covalence strongly shortens Fe${\sp 2+}$-X, Co${\sp 2+}$-X, Ni${\sp 2+}$-X, Mn${\sp 2+}$-X, Cu${\sp +}$-X, Ag${\sp +}$-X, and M-H${\sp -}$ bonds as the electronegativity of X or M decreases. Smaller effects are seen for Zn${\sp 2+}$-X, Cd${\sp 2+}$-X, In${\sp 2+}$-X, pb${\sp 2+}$-X, and TI${\sp +}$-X. Bonds with delocalized electrons and therefore metallic character, {\it e.g.} Sm-S, V-S, and Re-O, are significantly shorter than similar bonds with localized electrons.},
author = {Shannon, R D},
doi = {10.1107/S0567739476001551},
journal = {Acta Crystallogr. Sect. A},
month = {sep},
number = {5},
pages = {751--767},
title = {Revised effective ionic radii and systematic studies of interatomic distances in halides and chalcogenides},
