journal = {Proc. Natl. Acad. Sci. - PNAS},
keywords = {Amino Acid,Amino Acid Sequence,Binding Sites,Biological Sciences,Calcium - metabolism,Calmodulin - chemistry,Calmodulin - metabolism,EF-hand,Models,Molecular,Molecular Sequence Data,PNAS Plus,Sequence Homology,calcium signaling,evolution,protein,structure},
number = {9},
pages = {E1216--E1225},
publisher = {National Academy of Sciences},
series = {PNAS Plus},
title = {Conserved Properties of Individual {Ca}2+-Binding Sites in Calmodulin},
volume = {113},
year = {2016}
}
