keywords = {CO2,ILs},
mendeley-tags = {CO2,ILs},
month = {sep},
number = {38},
pages = {12727--12735},
pmid = {19722532},
title = {Temperature Dependence of the Solubility of Carbon Dioxide in Imidazolium-Based Ionic Liquids.},
url = {},
volume = {113},
year = {2009}
}
