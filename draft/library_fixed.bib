doi = {10.1021/acs.jpcb.6b05397},
file = {::},

journal = {J. Phys. Chem. B},
month = {aug},
number = {30},
pages = {7488--7501},
title = {Alkyl Chain Length Dependence of the Dynamics and Structure in the Ionic Regions of Room-Temperature Ionic Liquids},
url = {},
volume = {120},
year = {2016}
}
