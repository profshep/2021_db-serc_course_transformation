pages = {424121},
pmid = {270353800023},
title = {Morphology and intermolecular dynamics of 1-alkyl-3-methylimidazolium bis{(trifluoromethane)sulfonyl}amide ionic liquids: structural and dynamic evidence of nanoscale segregation},
volume = {21},
year = {2009}
}
