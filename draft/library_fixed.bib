journal = {Phys. Chem. Chem. Phys.},
keywords = {Electric Impedance,Electrons,ILs,Molecular Dynamics Simulation,Proteins,Proteins: chemistry,Thermodynamics,Water,Water: chemistry,charge transfer,ionicity},
mendeley-tags = {ILs,charge transfer,ionicity},
month = {feb},
