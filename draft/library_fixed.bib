isbn = {1463-9076},

journal = {Phys. Chem. Chem. Phys.},
number = {31},
pages = {16651--8},
pmid = {24992959},
title = {Effect of ion structure on nanoscale friction in protic ionic liquids.},
url = {},
volume = {16},
year = {2014}
}
