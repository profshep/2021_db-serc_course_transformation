pages = {74510},
title = {Small Angle Neutron Scattering from {1-alkyl-3-methylimidazolium} Hexafluorophosphate Ionic Liquids ([{C}$_n$mim][{PF}$_6$], n=4, 6, and 8)},
volume = {133},
year = {2010}
