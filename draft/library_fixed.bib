@article{Yoon1975,
abstract = {Conformational energies of meso and racemic dyads of poly(methyl acrylate) have been computed as functions of skeletal bond rotations, the planar ester group being oriented perpendicular to the plane defined by the two adjoining skeletal bonds. Solvent interactions affect the relative energies of various conformations, as found for polystyrene. The conformational energy surfaces are similar to those for polystyrene, and the same two-state scheme with trans and gauche states at $\phi$t = 10° and $\phi$g= 110° is applied. The first-order statistical weight parameter is given by $\eta$ = 1.0(±0.1) exp(-E$\eta$/RT) where the preexponential factor is evaluated from the computed conformational energy surfaces. Similarly, the second-order parameters that represent CH2-CH2, CH2-R, and R-R interactions, respectively, are $\omega$ = 1.3(±0.1) exp(-1600/RT), $\omega$′ = 1.4(±0.1) exp(-1600/RT), and $\omega$″ = 1.2(±0.1) exp(-E$\omega$″/RT). The energies E$\eta$ and E$\omega$″, being affected by solvent interactions, cannot be reliably estimated from energy calculations. Agreement with experimental dimensions of the PMA chain and stereochemical equilibria in the dimeric and trimeric oligomers requires that E$\eta$ = -300 ± 100 cal mol-1 and E$\omega$″ = 1500 ± 300 cal mol-1. Calculated values of the NMR-coupling parameter $\Delta$J for oligomers and for isotactic PMA are consistent with experimental results. {\textcopyright} 1975, American Chemical Society. All rights reserved.},
author = {Yoon, D. Y. and Suter, U. W. and Sundararajan, P. R. and Flory, P. J.},
