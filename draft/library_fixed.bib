journal = {J. Phys. Chem. Lett.},
number = {2},
pages = {484--488},
title = {Coordination-Dependent Spectroscopic Signatures of Divalent Metal Ion Binding to Carboxylate Head Groups: H2- and He-Tagged Vibrational Spectra of M2+{\textperiodcentered}RCO2¯ (M = Mg and Ca, R = −CD3, −CD2CD3) Complexes},
volume = {8},
