journal = {PLoS One},
keywords = {Animals,Binding Sites,Calcium,Calcium: metabolism,Chloride Channels,Chloride Channels: genetics,Chloride Channels: metabolism,Chloride Channels: physiology,Computer Simulation,Electrophysiological Processes,Eye Proteins,Eye Proteins: genetics,Eye Proteins: metabolism,Eye Proteins: physiology,Humans,Mice,Models, Molecular,Mutation,Patch-Clamp Techniques,Sequence Homology, Amino Acid,Thrombospondin 1,Thrombospondin 1: chemistry},
number = {3},
