file = {:Users/sgr/mendeley/Kashyap et al. - 2013 - How does the ionic liquid organizational landscape change when nonpolar cationic alkyl groups are replaced by po.pdf:pdf},
isbn = {1520-5207 (Electronic)\r1520-5207 (Linking)},

journal = {J. Phys. Chem. B},
number = {4},
pages = {1130--1135},
pmid = {23259707},
title = {How does the ionic liquid organizational landscape change when nonpolar cationic alkyl groups are replaced by polar isoelectronic diethers?},
volume = {117},
year = {2013}
}
