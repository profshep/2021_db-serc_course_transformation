doi = {10.1063/1.1534580},

journal = {J. Chem. Phys.},
month = {feb},
number = {5},
pages = {2270--2278},
title = {Orientational relaxation and vibrational excitation transfer in methanol–carbon tetrachloride solutions},
url = {},
volume = {118},
year = {2003}
}
