isbn = {1463-9076},

journal = {Phys. Chem. Chem. Phys.},
number = {22},
pages = {8821},
pmid = {23640471},
title = {Free volume in ionic liquids: a connection of experimentally accessible observables from PALS and PVT experiments with the molecular structure from XRD data},
