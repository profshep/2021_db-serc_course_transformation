@article{Keith2012,
abstract = {The role of pyridinium cations in electrochemistry has been believed known for decades, and their radical forms have been proposed as key intermediates in modern photoelectrocatalytic CO(2) reduction processes. Using first-principles density functional theory and continuum solvation models, we have calculated acidity constants for pyridinium cations and their corresponding pyridinyl radicals, as well as their electrochemical redox potentials. Contrary to previous assumptions, our results show that these species can be ruled out as active participants in homogeneous electrochemistry. A comparison of calculated acidities and redox potentials indicates that pyridinium cations behave differently than previously thought, and that the electrode surface plays a critical (but still unknown) role in pyridinium reduction. This work substantially alters the mechanistic view of pyridinium-catalyzed photoelectrochemical CO(2) reduction.},
