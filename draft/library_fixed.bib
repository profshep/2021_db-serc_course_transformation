keywords = {Ca2+,Ca2+-binding S100 proteins,Cu2+,EF-hand,Protein structures,Zn2+},
number = {4},
pages = {383--397},
pmid = {10191501},
title = {New perspectives on s100 proteins: A multi-functional Ca2+-, Zn2+- and Cu2+-binding protein family},
volume = {11},
year = {1998}
}
