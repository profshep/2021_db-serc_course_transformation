file = {::},

journal = {Phys. Chem. Chem. Phys.},
number = {17},
pages = {10889--10897},
publisher = {Royal Society of Chemistry},
title = {Effect of ion-ligand binding on ion pairing dynamics studied by two-dimensional infrared spectroscopy},
url = {},
volume = {19},
year = {2017}
}
