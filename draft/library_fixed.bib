journal = {Mol. Biosyst.},
number = {1},
pages = {215--224},
pmid = {20024083},
title = {The influence of EDTA and citrate anticoagulant addition to human plasma on information recovery from NMR-based metabolic profiling studies},
volume = {6},
year = {2009}
}
