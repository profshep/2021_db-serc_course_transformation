@article{saikJCP-04,
abstract = {Photoinduced electron transfer and geminate recombination are studied for the systems rhodamine 3B (R3B+)(R3B+) and rhodamine 6G (R6G+),(R6G+), which are cations, in neat neutral N,N-dimethylanilineN,N-dimethylaniline (DMA). Following photoexcitation of R3B+R3B+ or R6G+R6G+ (abbreviated as R+),R+), an electron is transferred from DMA to give the neutral radical R and the cation DMA+.DMA+. Because the DMA hole acceptor is the neat solvent, the forward transfer rate is very large, ∼5×1012 s−1.∼5×1012 s−1. The forward transfer is followed by geminate recombination, which displays a long-lived component suggesting several percent of the radicals escape geminate recombination. Spectrally resolved pump–probe experiments are used in which the probe is a “white” light continuum, and the full time-dependent spectrum is recorded with a spectrometer/charge-coupled device. Observations of stimulated emission (excited state decay—forward electron transfer), the R neutral radical spectrum, and the DMA+DMA+ radical cation spectrum as well as the ground-state bleach recovery (geminate recombination) make it possible to unambiguously follow the electron transfer kinetics. Theoretical modeling shows that the long-lived component can be explained without invoking hole hopping or spin-forbidden transitions.},
author = {Saik, V. O. and Goun, A. A. and Fayer, M. D.},
