@article{Dhunga2016,
abstract = {Ionic liquids with cyano anions have long been used because of their unique combination of low-melting temperatures, reduced viscosities and increased conductivities. Recently we have shown that cyano anions in ionic liquids are particularly interesting for their potential use as electron donors to excited state photo-acceptors (J. Phys. Chem. B, 2015, 119, 14790- 14799). Here we report on bulk structural and quantum mechanical results for a series of ionic liquids based on the 1-ethyl-3-methylimidazolium cation, paired with the following five cyano anions, SeCN−, SCN−, N(CN)− 2 , C(CN)− 3 , and B(CN)− 4 . By combining molecular dynamics simulations, high-energy wide-angle X-ray scattering measurements and periodic boundary condition DFT calculations we are able to obtain a comprehensive description of liquid landscape as well as the nature of HOMO-LUMO states for these in the condensed phase. Features in the structure functions for these ILs are somewhat different than the commonly observed adjacency, charge-charge and polarity peaks, especially for the bulkiest B(CN)− 4 anion. While the other four cyano-anion ILs present an anionic HOMO, the one for Im+ 2,1/B(CN)− 4 is cationic.},
author = {Dhungana, Kamal B. and Faria, Luiz F. O. and Wu, Boning and Liang, Min and Ribeiro, Mauro C. C. and Margulis, Claudio J. and Castner, Jr., Edward W.},
